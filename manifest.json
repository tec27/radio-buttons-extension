{
  "name": "radio-buttons",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Connect to a remote server and allow it to control a local Google Play Music client.",
  "homepage_url": "http://tec27.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://play.google.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://play.google.com/music/listen*"
      ],
      "js": [
        "js/radio-buttons.js"
      ]
    }
  ]
}
